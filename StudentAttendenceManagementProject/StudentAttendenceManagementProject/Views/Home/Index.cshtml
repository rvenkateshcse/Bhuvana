
@{
    ViewBag.Title = "Login";
}
@using (Html.BeginForm("Index", "Home", FormMethod.Post))
{
    @Html.Label("UserName:")
    @Html.TextBox("Un")
    <br />
    <br />
    <br />
    @Html.Label("Password:")
    @Html.TextBox("Pwd")
    <br />
    <br />
    <br />
    @Html.Label("Role:")
    @Html.TextBox("Rol")
    <br />
    <br />
    <br />
    <input type="submit" value="Login" />
    @Html.ActionLink("Register", "Register", "Registration", null, null);

}
