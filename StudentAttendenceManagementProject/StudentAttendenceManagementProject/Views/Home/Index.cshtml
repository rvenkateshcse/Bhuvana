@model StudentAttendenceManagementProject.Models.StudentModel
@{
    ViewBag.Title = "Login";
}

@using (Html.BeginForm("Index", "Home", FormMethod.Post))
{
    @Html.Label("UserName:")
    @Html.TextBoxFor(m => m.Un)
    <br />
    <br />
    <br />
    @Html.Label("Password:")
    @Html.TextBoxFor(m => m.Pwd)
    <br />
    <br />
    <br />
    @Html.Label("Role:")
    @Html.TextBoxFor(m => m.Rol)
    <br />
    <br />
    <br />
    <input type="submit" value="Login" />
    <input type="button" value="Register" onclick="@("window.location.href='"+@Url.Action("Register","Registration")+"'");" />

}
